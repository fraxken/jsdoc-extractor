{
  "name": "jsdoc-extractor",
  "version": "1.0.0",
  "description": "JSDoc annotations extractor",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "ava --verbose",
    "coverage": "nyc ava",
    "report": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fraxken/jsdoc-extractor.git"
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "GENTILHOMME.Thomas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fraxken/jsdoc-extractor/issues"
  },
  "homepage": "https://github.com/fraxken/jsdoc-extractor#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@slimio/eslint-config": "^2.0.3",
    "@slimio/is": "^1.5.0",
    "ava": "^0.25.0",
    "eslint": "^5.15.3",
    "husky": "^1.3.1",
    "nyc": "^13.3.0",
    "pkg-ok": "^2.3.1"
  }
}
